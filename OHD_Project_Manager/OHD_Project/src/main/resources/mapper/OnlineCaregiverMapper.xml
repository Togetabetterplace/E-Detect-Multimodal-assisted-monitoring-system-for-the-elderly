<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 在你的 XML 文件中 -->
<mapper namespace="com.ohd_project.ohd_project.mapper.OnlineCaregiverMapper">
    <select id="findOnlineCaregiverById" resultType="com.ohd_project.ohd_project.entity.Online_Caregiver">
        <!-- 这里是实际的 SQL 查询语句 -->
        SELECT * from online_caregiver WHERE caregiverId = #{caregiverId};
